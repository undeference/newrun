# path to directory that will be used for storing pid files
_PIDPATH="/var/tmp"
# path to directory where the server configuration files are located
_CONFIGPATH="$_PATH/conf"

# path to "functions" file
_FUNCTIONSPATH="$_PATH/functions"
# path of "exec" directory
_EXECPATH="$_PATH/exec"
# path of "defs" directory
_DEFPATH="$_PATH/defs"
# path of "plugins" directory (blank to disable plugins)
_PLUGINPATH="$_PATH/plugins"
